{
	"name": "har-sanitizer",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev:serve": "npx vite serve",
		"dev:wrangler": "npx wrangler pages dev --port 3001 --proxy 3000",
		"dev": "concurrently --kill-others \"npm run dev:serve\" \"npm run dev:wrangler\"",
		"build": "tsc && vite build",
		"lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
		"prettier": "prettier . --write",
		"preview": "vite preview"
	},
	"dependencies": {
		"@radix-ui/react-portal": "^1.0.4",
		"@radix-ui/react-visually-hidden": "^1.0.3",
		"class-variance-authority": "^0.7.0",
		"clsx": "^2.0.0",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"tailwind-merge": "^1.14.0",
		"tiny-invariant": "^1.3.1"
	},
	"devDependencies": {
		"@cloudflare/workers-types": "^4.20231016.0",
		"@types/har-format": "^1.2.14",
		"@types/react": "^18.2.15",
		"@types/react-dom": "^18.2.7",
		"@typescript-eslint/eslint-plugin": "^6.0.0",
		"@typescript-eslint/parser": "^6.0.0",
		"@vitejs/plugin-react": "^4.0.3",
		"autoprefixer": "^10.4.16",
		"concurrently": "^8.2.2",
		"eslint": "^8.45.0",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-react-refresh": "^0.4.3",
		"postcss": "^8.4.31",
		"prettier": "^3.0.3",
		"prettier-plugin-organize-imports": "^3.2.3",
		"tailwindcss": "^3.3.4",
		"typescript": "^5.0.2",
		"vite": "^4.4.5",
		"vitest-environment-miniflare": "^2.14.2",
		"wrangler": "^3.14.0"
	}
}
